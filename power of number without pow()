//To check power of number without using pow()
#include<stdio.h>
#include<conio.h>
//#include<stdlib.h>
void main()
{
int num,sum=0,pow;
clrscr();
printf("Enter the number : ");
scanf("%d",&num);
printf("Enter how much power(exponent) of number you want: ");
scanf("%d",&pow);
for(int i=2; i<=pow; i++)
{
	num=num*num;
}
printf("Resultant value is %d",num);
getch();
}
