#include<stdio.h>
#include<conio.h>
#include<stdlib.h>

void main()
{
int num;
clrscr();
printf("Enter the number to check prime or not :");
scanf("%d",&num);
if(num<2){
	 printf("Number is not prime");
	 getch();
	 exit(0);
	}
for(int i =2; i<num ; i++)
{
	if(num%i==0)
	{
		printf("Number is not prime");
		break;
	}
}
if(num==i)
{
	printf("Number is prime");
}

getch();
}
