#include<stdio.h>
#include<conio.h>

void main()
{
char ch;
clrscr();
printf("Enter the character to check character is uppercase,lowercase or special symbol:");
scanf("%c",&ch);
if(ch>=65 && ch<=90)
	printf("Character is Uppercase");
else if(ch>=97 && ch<=122)
	printf("Character is Lowercase");
else
	printf("Special Symbol");

getch();

}
