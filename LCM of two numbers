#include<stdio.h>
#include<conio.h>
void main()
{
int num1,num2;
clrscr();
printf("Enter the Number 1 & Number 2 to check their HCF ");
scanf("%d%d",&num1,&num2);
int max=num1>num2?num1:num2;
while(1)
{
    if((max%num1==0)&&(max%num2==0))
	       break;
    max++;
}
printf("Your HCF is %d",max);
getch();
}
