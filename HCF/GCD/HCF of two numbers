#include<stdio.h>
#include<conio.h>
void main()
{
int num1,num2;
int lcmfound=0;
clrscr();
printf("Enter the Number 1 & Number 2 to check their LCM ");
scanf("%d%d",&num1,&num2);
int min=num1<num2?num1:num2;
while(1)
{
  if((num1%min==0)&&(num2%min==0))
	break;
  min--;
}
printf("Your LCM is %d",min);
getch();
}
